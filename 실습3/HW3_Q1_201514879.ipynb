    "answer = 3\n",
    "numbers = list(map(int, input().split()))\n",
    "\n",
    "while(1):\n",
    "    count = 0\n",
    "    for number in numbers:\n",
    "        if answer % number == 0:\n",
    "            count += 1\n",
    "    if count >= 3:\n",
    "        break\n",
    "    answer += 1\n",
    "    \n",
    "print(answer)"
   ]
  }
